<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LeftAndRightView">
        <attr name="showText" format="boolean" />
        <attr name="labelPosition" format="enum">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="ImageSelectView">
        <attr name="imageselect_hspace" format="dimension" />
        <attr name="imageselect_vspace" format="dimension" />
    </declare-styleable>
    <declare-styleable name="NullDataView">
        <attr name="nulldataview_border_space" format="dimension" />
        <attr name="nulldataview_top_space" format="dimension" />
    </declare-styleable>
</resources>